      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.linear_model import LinearRegression\n",
        "\n",
        "X = np.array([1, 2, 3, 4, 5, 6, 7]).reshape(-1, 1)\n",
        "y = np.array([45, 50, 60, 80, 85, 90, 95])\n",
        "\n",
        "\n",
        "model = LinearRegression()\n",
        "model.fit(X, y)\n",
        "\n",
        "new_hours = np.array([[3.5], [8]])\n",
        "predictions = model.predict(new_hours)\n",
        "\n",
        "print(f\"Prediction for 3.5 hours: {predictions[0]:.2f} marks\")\n",
        "print(f\"Prediction for 8.0 hours: {predictions[1]:.2f} marks\")\n",
        "\n",
        "plt.scatter(X, y, color='blue', label='Actual Data')\n",
        "plt.plot(X, model.predict(X), color='red', label='Regression Line')\n",
        "plt.scatter(new_hours, predictions, color='green', marker='X', s=100, label='Predictions')\n",
        "\n",
        "plt.xlabel(\"Study Hours\")\n",
        "plt.ylabel(\"Exam Marks\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "print(f\"Slope (m): {model.coef_[0]:.2f}\")\n",
        "print(f\"Intercept (c): {model.intercept_:.2f}\")"
      ]
    }
